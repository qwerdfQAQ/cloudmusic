.list-head{display: -webkit-box;display: -webkit-flex;display: flex; margin: 30rpx;}
.list-head-img{width: 264rpx;height: 264rpx; border-radius: 30rpx; overflow: hidden;position: relative;margin-right: 42rpx;}
.list-head-img image{ width: 100%; height: 100%;}
.list-head-img text{position: absolute;right: 8rpx;top: 8rpx;color: white; font-size: 26rpx;}
.list-head-text{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;color: #f0f2f7;}
.list-head-text view:nth-child(1){ color: white;font-size: 34rpx;}
.list-head-text view:nth-child(2){display: -webkit-box;display: -webkit-flex;display: flex;margin:20rpx 0; font-size: 24rpx;-webkit-box-align: center;-webkit-align-items: center;align-items: center;}
.list-head-text view:nth-child(2) image{width: 54rpx;height: 54rpx;border-radius: 50%;margin-right: 14rpx;}
.list-head-text view:nth-child(3){line-height: 34rpx; font-size: 22rpx;}
.list-share{width:330rpx;height: 74rpx;margin: 0 auto;background: rgba(0,0,0,0.3);
	border-radius: 37rpx;color: white;text-align: center;line-height: 74rpx;font-size: 28rpx;}
.list-share text{margin-right: 16rpx;}
.list-music{background-color: white; border-radius: 50rpx; margin-top: 40rpx;}
.list-music-head{height: 50rpx;margin: 30rpx 0 70rpx 22rpx;overflow: hidden;}
.list-music-head text:nth-child(1){ height: 50rpx;font-size: 50rpx;}
.list-music-head text:nth-child(2){font-size: 30rpx;margin: 0 10rpx 0 26rpx;}
.list-music-head text:nth-child(3){font-size: 26rpx; color: #b2b2b2;}
.list-music-item{display: -webkit-box;display: -webkit-flex;display: flex; margin: 0 32rpx 66rpx 46rpx;-webkit-box-align: center;-webkit-align-items: center;align-items: center;color: #959595;}
.list-music-top{width: 58rpx;font-size: 30rpx;line-height: 30rpx;}
.list-music-song{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.list-music-song view:nth-child(1){font-size: 28rpx; color: black; width: 70vw; white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.list-music-song view:nth-child(2){font-size: 20rpx; -webkit-box-align: center; -webkit-align-items: center; align-items: center;width: 70vw; white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.list-music-song view:nth-child(2) image{width: 32rpx;height: 20rpx;margin-right: 10rpx; -webkit-flex-shrink: 0; flex-shrink: 0;}
.list-music-item text{font-size: 50rpx; color: #c7c7c7;}

